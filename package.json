{
  "name": "product-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node esbuild/build.js",
    "preplan": "npm run build",
    "plan": "source .env && cd ./terraform/env/dev && terraform init --upgrade=true && terraform plan",
    "predeploy": "npm run plan",
    "deploy": "source .env && cd ./terraform/env/dev && terraform apply -auto-approve",
    "destroy": "source .env && cd ./terraform/env/dev && terraform destroy -auto-approve"
  },
